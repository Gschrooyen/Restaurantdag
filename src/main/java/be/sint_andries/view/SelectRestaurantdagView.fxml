<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="651.0" prefWidth="769.0" xmlns="http://javafx.com/javafx/9"
            fx:controller="be.sint_andries.controller.SelectRestaurantdagViewController">
    <children>
        <VBox layoutX="-3.0" layoutY="-1.0" prefHeight="651.0" prefWidth="769.0">
            <children>
                <TableView fx:id="tblRestaurantdag" onMouseClicked="#setActive" prefHeight="506.0" prefWidth="768.0">
                    <columns>
                        <TableColumn fx:id="colId" prefWidth="85.0" text="ID"/>
                        <TableColumn fx:id="colNaam" minWidth="0.0" prefWidth="387.0" text="Naam"/>
                        <TableColumn fx:id="colDatum" prefWidth="290.0" text="Datum"/>
                    </columns>
                </TableView>
                <HBox alignment="CENTER" prefHeight="146.0" prefWidth="607.0">
                    <children>
                        <Button fx:id="btnBack" mnemonicParsing="false" text="Back">
                            <HBox.margin>
                                <Insets right="30.0"/>
                            </HBox.margin>
                        </Button>
                        <Button fx:id="btnSelect" disable="true" mnemonicParsing="false" onAction="#selectRestaurantdag"
                                text="Selecteren"/>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
