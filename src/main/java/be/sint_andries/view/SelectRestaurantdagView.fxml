<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="be.sint_andries.controller.SelectRestaurantdagViewController">
   <center>
        <VBox prefHeight="651.0" prefWidth="769.0" BorderPane.alignment="CENTER">
            <children>
                <TableView fx:id="tblRestaurantdag" onMouseClicked="#setActive" prefHeight="506.0" prefWidth="768.0">
                    <columns>
                        <TableColumn fx:id="colId" prefWidth="85.0" text="ID" />
                        <TableColumn fx:id="colNaam" minWidth="0.0" prefWidth="353.59999084472656" text="Naam" />
                        <TableColumn fx:id="colDatum" prefWidth="323.99993896484375" text="Datum" />
                    </columns>
                </TableView>
                <HBox alignment="CENTER" prefHeight="146.0" prefWidth="607.0">
                    <children>
                        <Button fx:id="btnBack" mnemonicParsing="false" text="Back">
                            <HBox.margin>
                                <Insets right="30.0" />
                            </HBox.margin>
                        </Button>
                        <Button fx:id="btnSelect" disable="true" mnemonicParsing="false" onAction="#selectRestaurantdag" text="Selecteren" />
                    </children>
                </HBox>
            </children>
        </VBox>
   </center>
</BorderPane>
