<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="343.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="be.sint_andries.controller.HomeScreenController">
    <children>
        <Button fx:id="btnClose" layoutX="252.0" layoutY="360.0" mnemonicParsing="false" onMouseClicked="#close" text="afsluiten" />
        <VBox alignment="CENTER" layoutX="-1.0" prefHeight="337.0" prefWidth="343.0">
            <children>
                <Button fx:id="btnOverview" mnemonicParsing="false" onMouseClicked="#ChangeSceneToKlantOverview" text="Klanten overview">
                    <VBox.margin>
                        <Insets bottom="20.0" />
                    </VBox.margin>
                </Button>
                <Button fx:id="btnRestoOverview" mnemonicParsing="false" onMouseClicked="#ChangeSceneToRestaurantdagOverview" text="Restaurantdagen overview">
                    <VBox.margin>
                        <Insets bottom="20.0" />
                    </VBox.margin>
                </Button>
                <Button fx:id="btnGerecht" mnemonicParsing="false" onMouseClicked="#ChangeSceneToGerechtOverZicht" text="Gerechten Overview" />
                <Button fx:id="btnArchief" onAction="#ChangeSceneToArchief" text="A_rchief">
                    <VBox.margin>
                        <Insets top="20.0" />
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>
