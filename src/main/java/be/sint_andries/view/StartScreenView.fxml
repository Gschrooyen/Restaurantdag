<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<BorderPane minHeight="450.0" minWidth="350.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="be.sint_andries.controller.StartScreenViewController">
   <center>
      <VBox alignment="CENTER" prefHeight="150.0" prefWidth="248.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#changeSceneToAddRestaurantdag" text="Bewerk basisGegevens" />
            <Button mnemonicParsing="false" onAction="#changeSceneToKlantOverview" text="Overzicht inschrijvingen">
               <VBox.margin>
                  <Insets top="40.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#ChangeScenteToAddKlant" text="Nieuwe inschrijving">
               <VBox.margin>
                  <Insets bottom="50.0" top="40.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </center>
   <top>
      <Label fx:id="lblTitel" text="Title" BorderPane.alignment="CENTER">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
   </top>
</BorderPane>
